{
    "benchmarks": [
        {
            "name": "node-density-cni-networkpolicy",
            "workload": "kube-burner",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "node-density-cni-networkpolicy",
                "PODS_PER_NODE": "45",
                "NODE_COUNT": "120",
                "JOB_TIMEOUT": "18000",
                "QPS": "20",
                "BURST": "20",
                "STEP_SIZE": "30s",
                "LOG_LEVEL": "info",
                "LOG_STREAMING": "true",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true",
                "MUST_GATHER_EACH_TASK": "true",
                "CLEANUP_TIMEOUT": "1h",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }",
                "ERROR_ON_VERIFY": "false"
            }
        }
    ]
}
